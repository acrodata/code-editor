{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "code-editor": {
      "projectType": "library",
      "root": "projects/code-editor",
      "sourceRoot": "projects/code-editor/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/code-editor/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/code-editor/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/code-editor/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/code-editor/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/code-editor/**/*.ts",
              "projects/code-editor/**/*.html"
            ],
            "eslintConfig": "projects/code-editor/eslint.config.js"
          }
        }
      }
    },
    "dev-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/dev-app",
      "sourceRoot": "projects/dev-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/dev-app",
            "index": "projects/dev-app/src/index.html",
            "browser": "projects/dev-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/dev-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/dev-app/src/favicon.ico",
              "projects/dev-app/src/assets"
            ],
            "styles": [
              "./projects/dev-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "dev-app:build:production"
            },
            "development": {
              "buildTarget": "dev-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "dev-app:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/dev-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/dev-app/src/favicon.ico",
              "projects/dev-app/src/assets"
            ],
            "styles": [
              "projects/dev-app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/dev-app/**/*.ts",
              "projects/dev-app/**/*.html"
            ],
            "eslintConfig": "projects/dev-app/eslint.config.js"
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {
            "baseHref": "/code-editor/",
            "remote": "origin"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}